/**
 * Module: andGate
 * 
 * TODO: Add module documentation
 */
module andGate(input in0, input in1, output out);
	
	always @(in0 or in1) begin
		if ((in0 == 1 ) && (in1 == 1))
				out = 1; 
		else 
			out = 0;
		end

endmodule


